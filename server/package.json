{
  "name": "habit-tracker-fullstack",
  "version": "2.0.0",
  "description": "Full-stack habit tracker with advanced features for coursework",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:debug": "nodemon --inspect server.js",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "docker:build": "docker build -t habit-tracker .",
    "docker:run": "docker run -p 5000:5000 --env-file .env habit-tracker",
    "docker:compose": "docker-compose up",
    "docker:compose:build": "docker-compose up --build",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "seed": "node scripts/seed.js",
    "seed:test": "node scripts/seed-test-data.js",
    "migrate": "node scripts/migrate.js",
    "backup": "node scripts/backup.js",
    "healthcheck": "node healthcheck.js"
  },
  "keywords": [
    "habit-tracker",
    "fullstack",
    "nodejs",
    "express",
    "mongodb",
    "jwt",
    "docker",
    "coursework"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "dotenv": "^16.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^6.10.0",
    "winston": "^3.9.0",
    "morgan": "^1.10.0",
    "validator": "^13.9.0",
    "node-schedule": "^2.1.1",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "socket.io": "^4.6.1",
    "nodemailer": "^6.9.3",
    "moment": "^2.29.4",
    "chart.js": "^4.3.0",
    "uuid": "^9.0.0",
    "csv-parser": "^3.0.0",
    "pdfkit": "^0.13.0",
    "exceljs": "^4.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.8",
    "faker": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diku9719/habit-tracker-fullstack.git"
  },
  "bugs": {
    "url": "https://github.com/diku9719/habit-tracker-fullstack/issues"
  },
  "homepage": "https://github.com/diku9719/habit-tracker-fullstack#readme"
}